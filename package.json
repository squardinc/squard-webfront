{
  "name": "squard",
  "private": true,
  "version": "0.0.1",
  "author": "Squard, Inc.",
  "license": "MIT",
  "scripts": {
    "build:tailwind": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind",
    "build": "gatsby build --prefix-paths",
    "dev": "gatsby develop --https",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "tslint": "tslint \"src/**/*.ts[x]\" --fix",
    "lint": "npm run format && npm run tslint",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --watch",
    "test-once": "jest  --coverage",
    "e2e": "node_modules/.bin/cypress open",
    "e2e-headless": "node_modules/.bin/cypress run",
    "deploy-s3": "gatsby-plugin-s3 deploy --yes",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
