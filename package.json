    "prettier": "^2.1.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "serverless-offline": "^6.7.0",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2",
    "wait-on": "^5.2.0"
